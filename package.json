{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Basic React Boilerplate",
  "main": "index.js",
  "scripts": {
    "build-babel": "npm run build-babel-server && npm run build-babel-client",
    "build-babel-server": "babel src/server --out-dir ./dist",
    "build-bable-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/index.js",
    "test": "jest ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shumai9/react-boilerplate"
  },
  "keywords": [
    "Node",
    "React"
  ],
  "author": "Shumai OA",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/shumai9/react-boilerplate/issues"
  },
  "homepage": "https://github.com/shumai9/react-boilerplate#readme",
  "dependencies": {
    "express": "^4.16.4",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "webpack": "^4.24.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "jest": "^23.6.0"
  }
}
